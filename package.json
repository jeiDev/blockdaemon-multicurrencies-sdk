{
   "name": "core-ubiquity-blockdaemon",
   "version": "1.0.0",
   "description": "",
   "main": "./src/index.ts",
   "scripts": {
      "dev": "nodemon",
      "build": "tsc -b",
      "typeorm": "ts-node ./node_modules/typeorm/cli.js -d ./src/database/connection/index.ts",
      "migration:generate": "npm run typeorm migration:generate ./src/database/migrations/%npm_config_name%",
      "migration:create": "typeorm migration:create ./src/database/migrations/%npm_config_name%",
      "migration:show": "npm run typeorm migration:show",
      "migration:run": "npm run typeorm migration:run",
      "model:create": "ts-node ./node_modules/typeorm/cli.js entity:create ./src/database/entities/%npm_config_name%",
      "subscriber:create": "typeorm subscriber:create ./src/database/subscribers/%npm_config_name%",
      "db:seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -n ./src/database/connection/ormconfig.ts",
      "start": "ts-node src/index.ts"
   },
   "keywords": [],
   "author": "Media Revolution",
   "license": "ISC",
   "devDependencies": {
      "@types/cors": "^2.8.12",
      "@types/express": "^4.17.13",
      "@types/express-session": "^1.17.4",
      "@types/node": "^16.11.10",
      "@types/ws": "^8.5.3",
      "nodemon": "^2.0.18",
      "ts-node": "10.7.0",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "axios": "^0.27.2",
      "body-parser": "^1.20.0",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.13.2",
      "cors": "^2.8.5",
      "dotenv": "^16.0.1",
      "express": "^4.18.1",
      "express-session": "^1.17.3",
      "helmet": "^5.1.0",
      "pg": "^8.7.3",
      "reflect-metadata": "^0.1.13",
      "typeorm": "^0.3.7",
      "typeorm-seeding": "^1.6.1",
      "ws": "^8.8.0"
   }
}
